---
import type { Translation } from "@/i18n";

interface Props {
    t: Translation;
}

const { t } = Astro.props;
---

<div class="text-center flex flex-col gap-6 items-center max-w-2xl mx-auto pt-10">
    <h1 class="text-xl font-bold">{t.pricing.title}</h1>
    <p>{t.pricing.description}</p>
</div>
