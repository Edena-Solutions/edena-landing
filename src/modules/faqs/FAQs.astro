---
import { getEntry } from 'astro:content';
import type { Translation } from '@/i18n';

import FAQAccordion, { type FAQItem } from '@/components/FAQAccordion.tsx';

interface Props {
    t: Translation
}

const {
    t,
} = Astro.props;

const faqEntry = await getEntry('faqs', 'faqspage');
---

<FAQAccordion
    faqs={faqEntry?.data.faqs || []} 
    t={t}
    client:load
/>